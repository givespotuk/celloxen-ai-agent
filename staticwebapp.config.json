{
  "routes": [
    {
      "route": "/login.html",
      "rewrite": "/login.html"
    },
    {
      "route": "/setup-test.html",
      "rewrite": "/setup-test.html"
    },
    {
      "route": "/dashboard/admin.html",
      "rewrite": "/dashboard/admin.html"
    },
    {
      "route": "/dashboard/clinic.html",
      "rewrite": "/dashboard/clinic.html"
    },
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.{css,js,json,png,jpg,gif,ico,svg}"]
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization"
  }
}
